<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="helpdesk_ticket_tree" model="ir.ui.view">
        <field name="name">view.helpdesk_ticket.tree</field>
        <field name="model">helpdesk.ticket</field>
        <field name="arch" type="xml">
            <tree string="Ticket">
                <field name="name"/>
                <field name="date"/>
                <field name="state"/>
            </tree>
        </field>
    </record>

</odoo>